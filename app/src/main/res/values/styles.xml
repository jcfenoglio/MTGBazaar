<resources>

    <!-- Base application theme. -->
    <style tradeName="AppTheme" parent="Theme.AppCompat.Light.DarkActionBar">
        <!-- Customize your theme here. -->
        <item tradeName="colorPrimary">@color/colorPrimary</item>
        <item tradeName="colorPrimaryDark">@color/colorPrimaryDark</item>
        <item tradeName="colorAccent">@color/colorAccent</item>
    </style>

    <style tradeName="AppTheme.NoActionBar">
        <item tradeName="windowActionBar">false</item>
        <item tradeName="windowNoTitle">true</item>
    </style>

    <style tradeName="AppTheme.AppBarOverlay" parent="ThemeOverlay.AppCompat.Dark.ActionBar" />

    <style tradeName="AppTheme.PopupOverlay" parent="ThemeOverlay.AppCompat.Light" />

</resources>
